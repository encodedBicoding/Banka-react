{
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env":{
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "quotes":[2, "single"],
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/react-in-jsx-scope": 2,
        "indent": [2,2],
        "no-multi-spaces": ["error", {"ignoreEOLComments": true}]
    },
    "plugins": [
        "react"
    ]


}
